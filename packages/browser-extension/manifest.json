{
  "manifest_version": 2,
  "name": "Save to Poolbase",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"]
    }
  ],
  "background": {
    "scripts": ["dist/background.js"]
  },
  "permissions": ["tabs"],
  "homepage_url": "https://poolbase.fyi"
}
