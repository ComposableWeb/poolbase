{
  "name": "@poolbase/landing-page",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "postbuild": "cpx \"public/**/*.*\" \"../../dist/landing/public\" -C",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "release": {
    "branch": "master",
    "extends": "semantic-release-monorepo",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "cpx": "1.5.0",
    "semantic-release": "^17.0.7",
    "semantic-release-monorepo": "^7.0.2"
  },
  "keywords": [],
  "author": "Andreas Adam <andreas.sahle@gmail.com> (https://pixelmord.de/)",
  "license": "MIT",
  "dependencies": {
    "gatsby": "^2.21.4",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-theme-localization": "^1.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
