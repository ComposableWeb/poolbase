{
  "name": "@poolbase/app",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "NODE_ENV=development next dev",
    "dev": "yarn start",
    "serve": "NODE_ENV=production next start",
    "build": "ANALYZE=true NODE_ENV=production next build",
    "typecheck": "tsc",
    "lint": "eslint '.'"
  },
  "release": {
    "branch": "master",
    "extends": "semantic-release-monorepo",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "keywords": [
    "pwa",
    "firebase",
    "nextjs"
  ],
  "author": "Andreas Adam <andreas.adam@composableweb.com> (https://pixelmord.de/)",
  "license": "MIT",
  "dependencies": {
    "@next/bundle-analyzer": "^9.3.6",
    "@poolbase/common": "0.0.1",
    "@poolbase/design-system": "0.0.1",
    "consola": "^2.11.3",
    "dotenv": "8.2.0",
    "firebase": "^7.14.2",
    "lodash": "^4.17.15",
    "next": "^9.3.6",
    "next-compose-plugins": "^2.2.0",
    "next-pwa": "^2.3.3",
    "next-transpile-modules": "^3.3.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.8",
    "react-firebase-hooks": "^2.1.1",
    "react-firebaseui": "^4.1.0",
    "react-hook-form": "^5.6.1",
    "reakit": "^1.0.0-rc.2",
    "rosetta": "^1.1.0",
    "theme-ui": "^0.3.1",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@firebase/testing": "^0.19.2",
    "@google-cloud/firestore": "^3.7.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/cookie-session": "2.0.39",
    "@types/cpx": "1.5.0",
    "@types/eslint": "^6.8.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.6",
    "@types/formidable": "^1.0.31",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.4",
    "@types/prettier": "^2.0.0",
    "@types/puppeteer": "^2.0.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/rimraf": "^3.0.0",
    "@types/semantic-release": "^15.13.1",
    "@types/theme-ui": "^0.3.0",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "cpx": "1.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.18.0",
